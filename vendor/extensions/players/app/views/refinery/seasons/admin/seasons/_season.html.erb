<li class='clearfix record <%= cycle("on", "on-hover") %>' id="<%= dom_id(season) -%>">
  <span class='title'>
    <%= season.title %>

  </span>
  <span class='actions'>
    
    <%= link_to refinery_icon_tag("application_go.png"), refinery.seasons_season_path(season),
        :title => t('.view_live_html'),
        :target => "_blank" %>
    
    <%= link_to refinery_icon_tag("application_edit.png"), refinery.edit_seasons_admin_season_path(season),
         :title => t('.edit') %>
    <%= link_to refinery_icon_tag("delete.png"), refinery.seasons_admin_season_path(season),
        :class => "cancel confirm-delete",
        :title => t('.delete'),
        :confirm => t('message', :scope => 'refinery.admin.delete', :title => season.title),
        :method => :delete %>
  </span>
</li>
